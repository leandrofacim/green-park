{
  "extends": "standard-with-typescript",
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "rules": {
     "@typescript-eslint/no-misused-promises": [
    "error",
    {
      "checksVoidReturn": false
    }
  ],
  "@typescript-eslint/no-namespace": "off",
  "@typescript-eslint/naming-convention": "off"
  }
}
